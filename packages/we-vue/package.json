{
  "name": "we-vue",
  "version": "3.0.0-alpha.2",
  "description": "Vue components based on Vue2.x and weui1.x",
  "keywords": [
    "vue",
    "weui",
    "we-vue",
    "component"
  ],
  "author": "tianyong90 <412039588@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tianyong90/we-vue.git"
  },
  "homepage": "https://wevue.org",
  "bugs": {
    "url": "https://github.com/tianyong90/we-vue/issues"
  },
  "license": "MIT",
  "main": "dist/we-vue.js",
  "module": "dist/we-vue.js",
  "style": "lib/we-vue-css/index.css",
  "types": "types/index.d.ts",
  "scripts": {
    "serve": "cross-env NODE_ENV=development webpack --config build/webpack.dev.conf.js --progress --hide-modules --colors --watch",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "lint:fix": "yarn run lint -- --fix",
    "build": "concurrently \"yarn build:dist --no-progress\" \"yarn build:lib\" -n \"dist,lib\" --kill-others-on-fail -r",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js --progress --hide-modules",
    "build:lib": "node build/build-lib.js",
    "build:we-vue-css": "gulp build --gulpfile packages/style/gulpfile.js --color --silent",
    "build:style-entry": "node build/build-style-entry.js",
    "build:we-vue": "cross-env NODE_ENV=production webpack --color --config build/webpack.build.conf.js && cross-env NODE_ENV=production webpack -p --color --config build/webpack.build.conf.js",
    "test": "jest --config=test/unit/jest.conf.js --runInBand",
    "test:watch": "jest --config=test/unit/jest.conf.js --watchAll",
    "coverage": "start test/unit/coverage/lcov-report/index.html",
    "release": "node build/release.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "vue-lazyload": "^1.2.0"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jest": "^23.3.8",
    "@types/node": "^10.12.1",
    "@types/webpack-env": "^1.13.6",
    "@vue/test-utils": "^1.0.0-beta.28",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.0",
    "cache-loader": "^2.0.0",
    "codecov": "^3.0.0",
    "commitizen": "^3.0.2",
    "conventional-changelog": "^3.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "faker": "^4.1.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^7.0.0",
    "git-branch": "^2.0.1",
    "husky": "^1.3.1",
    "inject-loader": "^4.0.0",
    "inquirer": "^6.0.0",
    "jest": "^23.0.0",
    "jest-css-modules": "^1.1.0",
    "jest-serializer-vue": "^2.0.0",
    "lint-staged": "^8.0.0",
    "listr": "^0.14.0",
    "lolex": "^3.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.14.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.0.0",
    "semver": "^5.3.0",
    "shelljs": "^0.8.0",
    "style-loader": "^0.23.0",
    "thread-loader": "^2.1.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.4",
    "uppercamelcase": "3.0.0",
    "url-loader": "^1.0.1",
    "vue": "^2.5.9",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.5.0",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.9"
  }
}
